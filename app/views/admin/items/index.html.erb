<div class="container" style="padding-bottom: 100px; margin-top: 140px;">
  <%= will_paginate(:renderer => WillPaginate::ActionView::Bootstrap4LinkRenderer) %>
    <h1>All items</h1>
      <ul>
        <% @items.each do |item| %>
          <li>
            <%= link_to item.name, admin_item_path(item) %>
            - <%= item.menu_type %>
          </li>
        <% end %>
      </ul>
      <%= render 'admin/items/partials/index_nav' %>
  <%= will_paginate(:renderer => WillPaginate::ActionView::Bootstrap4LinkRenderer) %>
</div>
