<% binding.pry %>
<div class="container" style="margin-top: 140px; margin-bottom: 30px; margin-left: 0px;">
  <% @cart.ordered_items.each do |cart_item| %>
<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Item's Name</th>
      <th scope="col">Price</th>
      <th scope="col">Quantity</th>
      <th scope="col">Update</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>cart_item.item.name</td>
      <td>Otto</td>
      <td>@mdo</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>Thornton</td>
      <td>@fat</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Larry</td>
      <td>the Bird</td>
      <td>@twitter</td>
    </tr>
  </tbody>
</table>
<% end %>
</div>
